{
  "expo": {
    "name": "FURE",
    "slug": "FURE",
    "version": "1.1.2",
    "orientation": "portrait",
    "icon": "./assets/icon_fure.png",
    "splash": {
      "image": "./app/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mypackage.fure",
      "buildNumber": "1.1.1",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.mypackage.fure",
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan",
      "versionCode": 13,
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "RECORD_AUDIO",
        "READ_PHONE_STATE",
        "READ_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "FU:RE ver 1.1.2"
  }
}
